<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: fff4ea;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.NisitController">
   <children>
      <Label layoutX="54.0" layoutY="44.0" text="Profile" textFill="#d7416e">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
      <Label layoutX="54.0" layoutY="143.0" text="ID:" textFill="#d7557c">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="54.0" layoutY="217.0" text="Name:" textFill="#d7557c">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="54.0" layoutY="298.0" text="Nickname:" textFill="#d7557c">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="nameLabel" layoutX="229.0" layoutY="217.0" text="{nameLabel}" textFill="#393939">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="nicknameLabel" layoutX="229.0" layoutY="298.0" text="{nicknameLabel}" textFill="#393939">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="studentIdLabel" layoutX="230.0" layoutY="143.0" text="{studentIdLabel}" textFill="#393939">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="739.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
